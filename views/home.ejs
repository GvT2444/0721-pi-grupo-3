<%-include("partes/head")%>

    <body>
        <link rel="stylesheet" href="css/geral.css">
        <link rel="stylesheet" href="css/home.css">
        <%-include("partes/header")%>
        <main>
            <div class="container">
                <img src="img/depositodemadeira.jpg" alt="">
            </div>
        </main>
        <section>
            <h3>Veja nossos produtos</h3>
            <div class="produtos">
                <% for(let p of produtos){ %>
                    <div class="produto" id="produto1">
                        <article>
                            <img src="img/<%= p.imagem%>" alt="<%= p.nome%>">
                            <h3><%= p.nome%></h3>
                            <h3>R$ <%= p.preco%></h3>
                        </article>
                    </div>
                <% } %>
            </div>
        </section>
        <%-include("partes/footer")%>
        <script src="home.js"></script>
    </body>

    </html>

